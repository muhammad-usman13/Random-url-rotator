{
  "manifest_version": 3,
  "name": "Random URL Rotator",
  "version": "2.0",
  "description": "Enhanced URL rotator with presets, validation, and statistics. Switches tabs through curated URL lists automatically.",
  "permissions": ["storage", "tabs", "alarms", "contextMenus"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Random URL Rotator"
  },
  "icons": {
    "16": "data:image/svg+xml;base64,",
    "48": "data:image/svg+xml;base64,"
  }
}